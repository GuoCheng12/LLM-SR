# LLM-SR Environment Variables Template
# Copy this file to .env and fill in your values

# OpenAI API Key (required for API-based usage)
API_KEY=your_openai_api_key_here

# CUDA Configuration
CUDA_VISIBLE_DEVICES=0

# Local LLM Server Configuration
LLM_SERVER_PORT=5000
LLM_SERVER_HOST=0.0.0.0

# Model Configuration
DEFAULT_MODEL_PATH=mistralai/Mixtral-8x7B-Instruct-v0.1

# Logging Configuration
LOG_LEVEL=INFO
TENSORBOARD_PORT=6006

# Experiment Configuration
MAX_SAMPLES=10000
TIMEOUT_SECONDS=100